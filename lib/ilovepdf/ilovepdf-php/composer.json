{
    "name": "ilovepdf/ilovepdf-php",
    "description": "iLovePDF Php Api",
    "type": "library",
    "homepage": "https://ilovepdf.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "ilovepdf",
            "email": "info@ilovepdf.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ilovepdf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "firebase/php-jwt": "^6.0",
        "guzzlehttp/guzzle": "^7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.21"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
